Features {
	Authentification
	Help
	PayingProcess
	CancelProcess
	ChossingProduct	
}

Combination {

	Authentification
	loop [" while UserDesire == TRUE "] {
	
		PayingProcess	
		
		alt ["CancelProcess  == True"] {
								opt ["Help IS True"] {
			Help	
		}
			CancelProcess
		
			else
			ChossingProduct
		}

	}

}